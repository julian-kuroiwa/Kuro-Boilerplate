{
  "name": "kuro-boilerplate",
  "version": "2.0.0",
  "description": "A simple boilerplate with Nunjucks, ES6, Gulp, SASS and Browsersync.",
  "main": ".eslintrc.js",
  "scripts": {
    "start": "rm -r dist && gulp",
    "build": "cross-env NODE_ENV=production gulp -- build",
    "lint": "./node_modules/.bin/eslint src/js/**/*.js",
    "fix": "./node_modules/.bin/eslint src --fix",
    "precommit": "npm run lint",
    "prepush": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/julian-kuroiwa/Kuro-Boilerplate.git"
  },
  "keywords": [
    "boilerplate"
  ],
  "author": "Julian Kuroiwa <julian.kuroiwa@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/julian-kuroiwa/Kuro-Boilerplate/issues"
  },
  "homepage": "https://github.com/julian-kuroiwa/Kuro-Boilerplate#readme",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "browser-sync": "^2.24.5",
    "cross-env": "^5.2.0",
    "del": "^3.0.0",
    "eslint": "^5.3.0",
    "fs": "0.0.1-security",
    "gulp": "^4.0.0",
    "gulp-autoprefixer": "^5.0.0",
    "gulp-if": "^2.0.2",
    "gulp-imagemin": "^4.1.0",
    "gulp-notify": "^3.2.0",
    "gulp-nunjucks-render": "^2.2.2",
    "gulp-plumber": "^1.2.0",
    "gulp-sass": "^4.0.1",
    "gulp-sourcemaps": "^2.6.4",
    "husky": "^0.14.3",
    "path": "^0.12.7",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "webpack": "^4.15.1",
    "webpack-cli": "^3.0.8",
    "webpack-stream": "^4.0.3"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "jquery": "^3.3.1"
  }
}
